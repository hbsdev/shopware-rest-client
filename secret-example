# Copy this file to ./secret and fill in your server data:

export SWAPI_PROTO=http
export SWAPI_SERVER=example.com
export SWAPI_BASEPATH=/
export SWAPI_USER=apiusername
export SWAPI_KEY=1234abcdefhjijklmnipqrstufwxyu6789

# Only locally:
export SWAPI_DEBUGLOG=log/swapi_debug.txt

# Starting with Shopware 5.0:
# injects additional dictionary values resembling the old SW4 data structures
# use 1 for yes and 0 for no
export SWAPI_COMPAT_SW4=1
